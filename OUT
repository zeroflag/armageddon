06B0:1304 ; XT BYE
06B0:1304 mov ah, 4C
06B0:1306 xor al, al
06B0:1308 int 21
06B0:130A 
-; DEFINE PRIMITIVE
-a 1400
06B0:1400 db 6,"branch"
06B0:1407 ; XT BRANCH
06B0:1407 add si, [si]
06B0:1409 lodsw
06B0:140A jmp ax
06B0:140C 
-; DEFINE PRIMITIVE
-a 1500
06B0:1500 db 7,"branch0"
06B0:1508 ; XT BRANCH0
06B0:1508 pop ax
06B0:1509 test ax, ax
06B0:150B jz 1407       ; code_branch
06B0:150F lodsw         ; SKIP THE ADDRESS 
06B0:1510 lodsw
06B0:1511 jmp ax
06B0:1513 
-; DEFINE PRIMITIVE
-a 1600
06B0:1600 db 6,"invert"
06B0:1607 ; XT INVERT
06B0:1607 pop ax
06B0:1608 not ax        ; each 1 is set to 0, and each 0 is set to 1
06B0:160A push ax
06B0:160B lodsw
06B0:160C jmp ax
06B0:160E 
-; DEFINE PRIMITIVE
-a 1700
06B0:1700 db 4,"exec"
06B0:1705 ; XT EXEC
06B0:1705 pop ax
06B0:1706 jmp ax
06B0:1708 
-; DEFINE PRIMITIVE
-a 1800
