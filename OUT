06B0:2802 xor ax, ax
06B0:2804 pop bx
06B0:2805 pop dx
06B0:2806 cmp dx, bx
06B0:2808 jnl 280D      ;@not_less
06B0:280A mov ax, FFFF
06B0:280D ;@not_less:
06B0:280D push ax    
06B0:280E lodsw
06B0:280F jmp ax
06B0:2811 
-a 2900
