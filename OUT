06B0:2208 dw 1C05, 1F06
06B0:220C dw 1104, BB90, 1B02
06B0:2212 dw 1104, 0400, 1B02
06B0:2218 ; jmp bx
06B0:2218 dw 1104, E3FF, 1B02
06B0:221E ; LIT    1  LIT   STATE !
06B0:221E dw 1104, 1, 1104, 0302, 1A02
06B0:2228 ; EXIT
06B0:2228 dw 2105
06B0:222A 
